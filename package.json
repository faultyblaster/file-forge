{
    "name": "templator",
    "displayName": "Templator",
    "description": "Templates for vscode",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.97.0"
    },
    "categories": [
        "Snippets",
        "Programming Languages"
    ],
    "publisher": "faultyblaster",
    "author": {
        "email": "faultyblaster@gmail.com",
        "name": "faultyblaster",
        "url": "https://x.com/faultyblasterxd"
    },
    "extensionPack": [],
    "repository": {
        "type": "git",
        "url": "https://github.com/faultyblaster/Templator"
    },
    "sponsor": {
        "url": "https:https://www.patreon.com/c/faultyblaster"
    },
    "keywords": [
        "snippet",
        "extension",
        "new file",
        "templates"
    ],
    "license": "MIT",
    "readme": "/README.md",
    "extensionKind": [
        "workspace"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "templator.newFile",
                "title": "Create new file",
                "category": "Templator"
            },
            {
                "command": "templator.createCSharpNamespace",
                "title": "Create C# namespace",
                "category": "Templator"
            },
            {
                "command": "templator.newCSFile",
                "title": "Create C# File",
                "category": "Templator"
            },
            {
                "command": "templator.newCSClass",
                "title": "Class",
                "category": "Templator"
            },
            {
                "command": "templator.newCSEnum",
                "title": "Enum",
                "category": "Templator"
            },
            {
                "command": "templator.newCSInterface",
                "title": "Interface",
                "category": "Templator"
            },
            {
                "command": "templator.newCSRecord",
                "title": "Record",
                "category": "Templator"
            },
            {
                "command": "templator.newCSStruct",
                "title": "Struct",
                "category": "Templator"
            },
            {
                "command": "templator.newCsGlobalUsings",
                "title": "Global Usings",
                "category": "Templator"
            },
            {
                "command": "templator.newTSFile",
                "title": "Create TS File",
                "category": "Templator"
            },
            {
                "command": "templator.newTSClass",
                "title": "Class",
                "category": "Templator"
            },
            {
                "command": "templator.newTSEnum",
                "title": "Enum",
                "category": "Templator"
            },
            {
                "command": "templator.newTSInterface",
                "title": "Interface",
                "category": "Templator"
            },
            {
                "command": "templator.newPYFile",
                "title": "Create PY File",
                "category": "Templator"
            },
            {
                "command": "templator.newPYMain",
                "title": "Main",
                "category": "Templator"
            },
            {
                "command": "templator.newPYRequirements",
                "title": "Requirements",
                "category": "Templator"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "templator.newFile",
                    "when": "explorerResourceIsFolder",
                    "group": "_fileCreation@0"
                },
                {
                    "command": "templator.createCSharpNamespace",
                    "when": "resourceExtname == .cs || resourceExtname == .fs}",
                    "group": "_dotnet"
                },
                {
                    "submenu": "file.creation.cs",
                    "group": "_fileCreation",
                    "when": "explorerResourceIsFolder && templator.showCSharpContext"
                },
                {
                    "submenu": "file.creation.ts",
                    "group": "_fileCreation",
                    "when": "explorerResourceIsFolder && templator.showTSContext"
                },
                {
                    "submenu": "file.creation.py",
                    "group": "_fileCreation",
                    "when": "explorerResourceIsFolder && templator.showPYContext"
                }
            ],
            "file.creation.cs": [
                {
                    "command": "templator.newCSFile",
                    "group": "_general@1"
                },
                {
                    "command": "templator.newCSClass",
                    "group": "basic@2"
                },
                {
                    "command": "templator.newCSEnum",
                    "group": "basic@2"
                },
                {
                    "command": "templator.newCSInterface",
                    "group": "basic@2"
                },
                {
                    "command": "templator.newCSRecord",
                    "group": "basic@2"
                },
                {
                    "command": "templator.newCSStruct",
                    "group": "basic@2"
                },
                {
                    "command": "templator.newCsGlobalUsings",
                    "group": "global@1"
                }
            ],
            "file.creation.ts": [
                {
                    "command": "templator.newTSFile",
                    "group": "_general@1"
                },
                {
                    "command": "templator.newTSClass",
                    "group": "basic@2"
                },
                {
                    "command": "templator.newTSEnum",
                    "group": "basic@2"
                },
                {
                    "command": "templator.newTSInterface",
                    "group": "basic@2"
                }
            ],
            "file.creation.py": [
                {
                    "command": "templator.newPYFile",
                    "group": "_general@1"
                },
                {
                    "command": "templator.newPYMain",
                    "group": "basic@2"
                },
                {
                    "command": "templator.newPYRequirements",
                    "group": "basic@2"
                }
            ],
            "commandPalette": [
                {
                    "command": "templator.newCSClass",
                    "when": "false"
                },
                {
                    "command": "templator.newCSEnum",
                    "when": "false"
                },
                {
                    "command": "templator.newCSInterface",
                    "when": "false"
                },
                {
                    "command": "templator.newCSRecord",
                    "when": "false"
                },
                {
                    "command": "templator.newCSStruct",
                    "when": "false"
                },
                {
                    "command": "templator.newTSClass",
                    "when": "false"
                },
                {
                    "command": "templator.newTSEnum",
                    "when": "false"
                },
                {
                    "command": "templator.newTSInterface",
                    "when": "false"
                },
                {
                    "command": "templator.newPYMain",
                    "when": "false"
                },
                {
                    "command": "templator.newPYRequirements",
                    "when": "false"
                }
            ]
        },
        "submenus": [
            {
                "id": "file.creation.cs",
                "label": "New C# file"
            },
            {
                "id": "file.creation.ts",
                "label": "New TypeScript file"
            },
            {
                "id": "file.creation.py",
                "label": "New Python file"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "npm run check-types && npm run lint && node esbuild.js",
        "watch": "npm-run-all -p watch:*",
        "watch:esbuild": "node esbuild.js --watch",
        "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
        "package": "npm run check-types && npm run lint && node esbuild.js --production",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "npm run compile-tests && npm run compile && npm run lint",
        "check-types": "tsc --noEmit",
        "lint": "eslint src",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/vscode": "^1.97.0",
        "@types/mocha": "^10.0.10",
        "@types/node": "20.x",
        "@typescript-eslint/eslint-plugin": "^8.22.0",
        "@typescript-eslint/parser": "^8.22.0",
        "eslint": "^9.19.0",
        "esbuild": "^0.24.2",
        "npm-run-all": "^4.1.5",
        "typescript": "^5.7.3",
        "@vscode/test-cli": "^0.0.10",
        "@vscode/test-electron": "^2.4.1"
    }
}
